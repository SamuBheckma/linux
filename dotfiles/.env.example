# .env.example - Copie para .env e preencha seus valores
# NUNCA commite arquivos .env reais no git

# Node/NPM
NODE_ENV=development

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=user
DB_PASSWORD=password
DB_NAME=database

# API Keys (nunca commite valores reais!)
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# Python
PYTHONUNBUFFERED=1
PYTHONPATH=.

# Docker
DOCKER_REGISTRY=docker.io
DOCKER_USERNAME=yourname

# Ports
PORT=3000
DEBUG_PORT=9229
